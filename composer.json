{
  "name": "themosis/framework",
  "description": "The Themosis framework.",
  "keywords": ["themosis", "framework", "wordpress"],
  "license": "GPL-2.0-or-later",
  "support": {
    "issues": "https://github.com/themosis/framework/issues",
    "source": "https://github.com/themosis/framework"
  },
  "authors": [
    {
      "name": "Julien Lamb√©",
      "email": "julien@themosis.com",
      "homepage": "https://www.themosis.com/"
    }
  ],
  "autoload": {
    "psr-4": {
      "Themosis\\": "src/"
    },
    "files": [
      "src/Foundation/helpers.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Themosis\\Tests\\": "tests/"
    }
  },
  "require": {
    "php": "^8.0",
    "composer/installers": "^1.9",
    "laravel/framework": "^8.81",
    "laravel/tinker": "^2.5",
    "laravel/ui": "^3.1",
    "league/fractal": "^0.19.0",
    "paragonie/sodium_compat": "^1.15",
    "predis/predis": "^1.1",
    "ramsey/uuid": "^4.1",
    "symfony/property-access": "^5.2",
    "twig/twig": " ^3.1"
  },
  "require-dev": {
    "beyondcode/laravel-dump-server": "^1.7",
    "friendsofphp/php-cs-fixer": "^3.5",
    "phpunit/phpunit": "^9.0",
    "symfony/var-dumper": "^5.2"
  },
  "scripts": {
    "test": "phpunit"
  },
  "config": {
    "sort-packages": true,
    "allow-plugins": {
      "composer/installers": true
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
